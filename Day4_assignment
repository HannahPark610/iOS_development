//=======================================
//problem 3.
//=======================================

    int num, count, min, max;
    
    printf("Enter min range:");
    scanf("%d", &min);
    
    printf("Enter max range:");
    scanf("%d", &max);
    
    count = 0;
    
    for(num=min+1; num<max; num++)
    {
        count=count+1;
    }
        
    printf("It's your between number: %d\n", count);
    
    
    return 0;
}
