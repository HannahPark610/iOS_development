#include <stdio.h>
#include <string.h>

struct fields
{
    char day[10];
    int month;
    float year;
    
};


int main(void) {
    
    char day;
    struct fields dates = {"thirtieth", 1, 2016.0};
    struct fields *ptr;
    
    ptr = &dates;
    
    printf("Dates of field: \n");
    printf(" Day is: %s \n ", ptr->day);
    printf(" Month is: %d \n ", ptr->month);
    printf(" Year is: %f \n ", ptr->year);
    
    return 0;
}
