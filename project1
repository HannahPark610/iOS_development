#include <stdio.h>

typedef struct student1
{
    char userName[50];
    char passWord[10];
    int studentId;
    char firstName[20];
    char lastName[20];
    char gender[5];
    int grade;
    char courseId[50];
    int mark;
    char address[100];
    int admission;
}student1;

student1 = {"user1", "password1"};

int main()
{
	printf("\nusername : %s",student1.userName);
    printf("\npassword : %s",student1.passWord);
	return 0;
}
